<?php

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_ENTITY_TYPE_presave()
 */
function pezbase_ui_node_presave(&$node) {
  \Drupal::messenger()->addMessage($node->getType());
  if ($node->getType() == 'stem') {
    // $title = separated list of attributes.
    $title = 'foo' . \rand();
    // Todo: check that the title is unique.
    $node->title = $title;
  }
}
